<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence
          http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd"
	version="2.1">

	<persistence-unit name="persistence">
	<class>com.springbook.entity.AbstractEntity</class>
	
	<class>com.springbook.entity.user.UserEntity</class>
		<class>com.springbook.entity.user.RoleEntity</class>
		<class>com.springbook.entity.user.ShipAddressEntity</class>
		<class>com.springbook.entity.user.ContactEntity</class>
		
		<class>com.springbook.entity.book.BookEntity</class>
		<class>com.springbook.entity.book.TagEntity</class>
		<class>com.springbook.entity.book.PublisherEntity</class>
		<class>com.springbook.entity.book.CategoryEntity</class>
		<class>com.springbook.entity.book.AuthorEntity</class>
		
		<class>com.springbook.entity.book.WareHouseEntity</class>
		<class>com.springbook.entity.book.BookWarehouseEntity</class>
		
		<class>com.springbook.entity.book.CommentEntity</class>

		<class>com.springbook.entity.order.OrderEntity</class>
		<class>com.springbook.entity.order.OrderDetailEntity</class>
		
		
		<class>com.springbook.entity.cart.CartEntity</class>
		<class>com.springbook.entity.cart.DetailCartEntity</class>
	
	
	</persistence-unit>

</persistence>